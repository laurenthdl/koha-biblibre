<?xml version="1.0" encoding="UTF-8"?>

<dom>
  <extract name="index">
    <xslt stylesheet="unimarc.xsl"/>
  </extract>
  <retrieve name="unimarc">
    <xslt stylesheet="identity.xsl"/>
  </retrieve>
  <!--- examples if you have a unimarc to dc -->
  <retrieve name="dc">
    <xslt stylesheet="unimarc2dc.xsl"/>
  </retrieve> 
  <!-- example if there was a way to convert from unimarc to marc21/usmarc via xsl  -->

  <retrieve name="marc21">
    <xslt stylesheet="unimarc2marc21.xsl"/>
  </retrieve>
  <input syntax="xml">
    <marc inputcharset="utf-8"/>
  </input>
</dom>
