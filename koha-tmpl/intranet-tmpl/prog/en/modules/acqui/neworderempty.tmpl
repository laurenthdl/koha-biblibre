                        $("#sort1").replaceWith(textbox);
                    }else{
                        $("#sort1").replaceWith(data);
                    }
                }
            });
            $.ajax({
                url: "/cgi-bin/koha/acqui/ajax-getauthvaluedropbox.pl",
