<script type="text/javascript">
//<![CDATA[
 $(document).ready(function() {
    $(".baskets").tablesorter({
        widgets : ['zebra'],
        headers: {
            6: {sorter: false},
        }
    });
    toggleReceivedBaskets();
 }); 

var AllBasketsShown = 1;
function toggleReceivedBaskets() {
    var lines = $(".baskets tbody > tr");
    var i = 0;
    if(AllBasketsShown){
        $(lines).each(function(){
            if($(this).hasClass("rcvd")){
                $(this).addClass("content_hidden");
            }else{
                $(this).removeClass("even");
                $(this).removeClass("odd");
                if(i%2 == 0){
                    $(this).addClass("even");
                }else{
                    $(this).addClass("odd");
                }
                i++;
            }
        });
        $("#showReceivedBaskets").html("Show all baskets");
        AllBasketsShown = 0;
    }else{
        $(lines).each(function(){
            $(this).removeClass("content_hidden");

            $(this).removeClass("even");
            $(this).removeClass("odd");
            if(i%2 == 0){
                $(this).addClass("even");
            }else{
                $(this).addClass("odd");
            }
            i++;
        });
        $("#showReceivedBaskets").html("Hide received baskets");
        AllBasketsShown = 1;
    }
}
 //]]>
</script>

